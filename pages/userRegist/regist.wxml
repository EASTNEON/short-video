<!--pages/userRegist/regist.wxml-->
<view>
  <view class='firseView'>
    <form bindsubmit='doRegist'>
      <view class='inputView'>
        <image src='../resource/images/user.png' class='nameImage'></image>
        <label class='loginLable'> 账号</label>
        <input name='username' class='inputText' placeholder='请输入账号'></input>
      </view>
      <view class='line'></view>
      <view class='inputView'>
        <image src='../resource/images/pwd.png' class='keyImage'></image>
        <label class='loginLable'> 密码</label>
        <input name='password' class='inputText' placeholder='请输入密码'></input>
      </view>
      <view class='line'></view>
        <button form-type='doRegist' class='RegistBtn'>注册</button>
        <button  class='goLoginBtn'>返回登录</button>
    </form>
  </view>
</view>
